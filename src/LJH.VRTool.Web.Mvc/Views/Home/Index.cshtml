@{
    ViewBag.Title = "后台首页";
    Layout = "~/Views/Shared/Krpano/_Layout.cshtml";
}
@using LJH.VRTool.Web.Views.Home.Components.AccountInfoComponent
@using LJH.VRTool.Web.Views.Home.Components.MenuComponent
@using LJH.VRTool.Web.Views.Home.Components.TopBarComponent
@using LJH.VRTool.Web.Views.Home.Components.FootBarComponent

<div id="wrapper">
    <!--左侧导航开始-->
    <nav class="navbar-default navbar-static-side" role="navigation">
        <div class="nav-close">
            <i class="fa fa-times-circle"></i>
        </div>
        <div class="sidebar-collapse">
            <ul class="nav" id="side-menu">
                @await Component.InvokeAsync(typeof(AccountInfoComponent))
                @await Component.InvokeAsync(typeof(MenuComponent))
            </ul>
        </div>
    </nav>
    <!--左侧导航结束-->
    <!--右侧部分开始-->
    <div id="page-wrapper" class="gray-bg dashbard-1">
        <div class="row border-bottom">
            <nav class="navbar navbar-static-top" role="navigation" style="margin-bottom: 0;">
                <div class="navbar-header">
                    <a class="navbar-minimalize minimalize-styl-2 btn btn-primary " href="#"><i class="fa fa-bars"></i> </a>
                </div>
                <ul class="nav navbar-top-links navbar-right">
                    <li class="hidden-xs">
                        <div class="btn-group roll-nav roll-right">
                          
                        </div>

                        @*<a href="index_v1.html" class="J_menuItem" data-index="0"><i class="fa fa-cart-arrow-down"></i> 购买</a>*@
                    </li>
                    <li class="dropdown hidden-xs">
                        <a class="right-sidebar-toggle" aria-expanded="false">
                             前台首页
                        </a>
                    </li>
                </ul>
            </nav>

        </div>
        <div class="row content-tabs">
            @await Component.InvokeAsync(typeof(TopBarComponent))
        </div>
        <div class="row J_mainContent" id="content-main">
            <iframe class="J_iframe" name="iframe0" width="100%" height="100%" src="" frameborder="0" data-id="index_v2.html"></iframe>
        </div>
        @await Component.InvokeAsync(typeof(FootBarComponent))
    </div>
    <!--右侧部分结束-->
</div>
